<%= render "layouts/header"%>
<br>
<br>
<%= form_for @entry, url:{action: 'update'} do |f|%>
  <%= f.label  "Title:", class: "labels"%>
  <br>
  <%= f.text_field :title, value: @entry.title%>
  <br>
  <br>
  <%= f.label "Content:", class: "labels"%>
  <br>
  <%= f.text_area :content, value: @entry.content %>
  <br>
  <br>
  <%=f.submit%>
<% end %>
